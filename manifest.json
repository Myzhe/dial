{
  "name": "Dial",
  "description": "It's a launcher of frequent websites that you define.",
  "version": "2.0",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://query.yahooapis.com; object-src 'self'"
}
